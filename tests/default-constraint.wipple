[intrinsic] Number : type
[intrinsic] String : type
[intrinsic] List : element => type
[intrinsic] Set : element => type

Collect : element collection => trait (element -> collection)
instance (Collect element (List element)) : _
instance (Collect element (Set element)) : _

-- Default and bound constraints are resolved in source code order
collect :: element -> output where (output :: List element) (Collect element output)
collect : _

_ : collect "hi"
_ : (collect "hi" :: Set _)
