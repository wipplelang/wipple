{
    "compilerOptions": {
        "target": "ES2020",
        "lib": ["ES2020"],
        "moduleResolution": "node",
        "module": "esnext",
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "strict": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "noEmit": true
    }
}
