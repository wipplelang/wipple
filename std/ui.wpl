[[no-implicit-use]]

[help "Use a UI element from a JavaScript file."]
[help-group "User Interface"]
with-ui :: Result => Text -> (UI -> Result) -> Result
with-ui : url f -> intrinsic "with-ui" url f

[help "Send a message to a UI element."]
[help-group "User Interface"]
message :: Input (Output : ()) => Text -> Input -> UI -> Output
message : message data ui -> intrinsic "message-ui" ui message data
