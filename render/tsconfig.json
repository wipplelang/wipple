{
    "compilerOptions": {
        "target": "esnext",
        "lib": ["esnext"],
        "moduleResolution": "nodenext",
        "module": "nodenext",
        "skipLibCheck": true,
        "strict": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "declaration": true,
        "emitDeclarationOnly": true,
        "declarationDir": "dist"
    }
}
