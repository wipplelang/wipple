-- Helper functions --

it :: A => A -> A
it : x -> x

just :: A B => A -> B -> A
just : x -> _ -> x

-- Output --

Show : A => trait (A -> Text)
instance Show Text : it
instance Show Number : n -> external "builtin" "number-to-text" n

show :: S where (Show S) => S -> ()
show : x -> external "builtin" "show" (Show x)

-- Math --

Add : Left Right Sum => trait (Left -> Right -> Sum)
instance Add Number Number Number : a -> b -> external "builtin" "add-numbers" a b
