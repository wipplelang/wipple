-- wipple run $FILE --lib library/src/foundation

remove-duplicates :: (List element) -> Sequence element where (Describe element) (Equal element) (Hash element)
remove-duplicates : do {
  seen : (,)
  filter (item -> (seen . (not contains?) item) and (do {
    set seen : seen . insert item
    True
  }))
}

numbers : 5, 5, 4, 3, 3, 3, 2, 1, 2, 3, 4, 5
numbers . remove-duplicates . each show
