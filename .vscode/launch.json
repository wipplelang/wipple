{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug VSCode Extension",
            "type": "extensionHost",
            "request": "launch",
            "args": ["--extensionDevelopmentPath=${workspaceFolder}/tools/vscode-extension"],
            "outFiles": ["${workspaceFolder}/tools/vscode-extension/dist/**/*.js"],
            "preLaunchTask": "${defaultBuildTask}"
        }
    ]
}
