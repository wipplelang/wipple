[[no-implicit-use]]

use "range"
use "list"
use "util"
use "sequence"

random-between :: Range Number -> Number
random-between : { min max } -> {
  (max >= min) . expect "`max` must be greater than `min`"
  intrinsic "random-number" min max
}

random-element :: Element => List Element -> Maybe Element
random-element : l -> l . nth (random-between (0 to (count l)))

random? :: () -> Boolean
random? : () -> random-between (0 to 2) = 1
