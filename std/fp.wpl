[[no-std]]

[help "Returns its input unmodified."]
[help-group "Utilities"]
it :: A => A -> A
it : x -> x

[help "Ignores its second input."]
[help-group "Utilities"]
just :: A => A -> _ -> A
just : x _ -> x

[operator Composition-Precedence]
[help "Call a function on an input."]
[help-group "Utilities"]
[help-template "{%input%} . {%function%}"]
. : syntax {
  'x . 'f -> ('f) ('x)
  'x . -> f -> f ('x)
  . 'f -> 'f
  . -> f x -> f x
}
