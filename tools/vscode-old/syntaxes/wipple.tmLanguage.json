{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "Wipple",
    "patterns": [
        {
            "name": "string",
            "match": "\"(?:[^\"\\\\]|\\\\.)*\""
        },
        {
            "name": "comment",
            "match": "--.*"
        },
        {
            "name": "constant.numeric",
            "match": "\\b-?[0-9]+(\\.[0-9]+)?\\b"
        },
        {
            "name": "punctuation",
            "match": "['\\/]"
        },
        {
            "name": "entity.name",
            "match": "\\b[^\\r\\n\\t \\(\\)\\[\\]\\{\\}'\"/]+\\b"
        }
    ],
    "scopeName": "source.wipple"
}
